#################################################################
#    Datatypes
#################################################################

###  http://www.opengis.net/citygml/2.0/core#RelativeToTerrain
core:RelativeToTerrain rdf:type rdfs:Datatype ;
                       rdfs:label "RelativeToTerrain"@en ;
                       skos:prefLabel "RelativeToTerrain"@en ;
                       owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                             owl:oneOf [ rdf:type rdf:List ;
                                                         rdf:first "entirelyAboveTerrain"^^xsd:string ;
                                                         rdf:rest [ rdf:type rdf:List ;
                                                                    rdf:first "entirelyBelowTerrain"^^xsd:string ;
                                                                    rdf:rest [ rdf:type rdf:List ;
                                                                               rdf:first "substantiallyAboveAndBelowTerrain"^^xsd:string ;
                                                                               rdf:rest [ rdf:type rdf:List ;
                                                                                          rdf:first "substantiallyAboveTerrain"^^xsd:string ;
                                                                                          rdf:rest [ rdf:type rdf:List ;
                                                                                                     rdf:first "substantiallyBelowTerrain"^^xsd:string ;
                                                                                                     rdf:rest [ rdf:type rdf:List ;
                                                                                                                rdf:first "temporarilyAboveAndBelowTerrain"^^xsd:string ;
                                                                                                                rdf:rest rdf:nil
                                                                                                              ]
                                                                                                   ]
                                                                                        ]
                                                                             ]
                                                                  ]
                                                       ]
                                           ] .


###  http://www.opengis.net/citygml/2.0/core#RelativeToWater
core:RelativeToWater rdf:type rdfs:Datatype ;
                     rdfs:label "RelativeToWater"@en ;
                     skos:prefLabel "RelativeToWater"@en ;
                     owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                           owl:oneOf [ rdf:type rdf:List ;
                                                       rdf:first "entirelyAboveWaterSurface"^^xsd:string ;
                                                       rdf:rest [ rdf:type rdf:List ;
                                                                  rdf:first "entirelyBelowWaterSurface"^^xsd:string ;
                                                                  rdf:rest [ rdf:type rdf:List ;
                                                                             rdf:first "substantiallyAboveAndBelowWaterSurface"^^xsd:string ;
                                                                             rdf:rest [ rdf:type rdf:List ;
                                                                                        rdf:first "substantiallyAboveWaterSurface"^^xsd:string ;
                                                                                        rdf:rest [ rdf:type rdf:List ;
                                                                                                   rdf:first "substantiallyBelowWaterSurface"^^xsd:string ;
                                                                                                   rdf:rest [ rdf:type rdf:List ;
                                                                                                              rdf:first "temporarilyAboveAndBelowWaterSurface" ;
                                                                                                              rdf:rest rdf:nil
                                                                                                            ]
                                                                                                 ]
                                                                                      ]
                                                                           ]
                                                                ]
                                                     ]
                                         ] .


###  http://www.w3.org/2001/XMLSchema#date
